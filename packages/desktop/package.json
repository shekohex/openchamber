{
  "name": "@openchamber/desktop",
  "version": "1.6.8",
  "private": true,
  "type": "module",
  "desktopPrerequisites": [
    "Rust stable toolchain (via rustup)",
    "Xcode Command Line Tools installed",
    "Tauri CLI installed (cargo install tauri-cli@^2)"
  ],
  "scripts": {
    "tauri": "tauri",
    "tauri:dev": "tauri dev --features devtools",
    "tauri:build": "tauri build",
    "tauri:android:init": "tauri android init --config src-tauri/tauri.android.conf.json",
    "tauri:android:dev": "tauri android dev --config src-tauri/tauri.android.conf.json",
    "tauri:android:build": "tauri android build --config src-tauri/tauri.android.conf.json",
    "tauri:ios:init": "tauri ios init --config src-tauri/tauri.ios.conf.json",
    "tauri:ios:dev": "tauri ios dev --config src-tauri/tauri.ios.conf.json",
    "tauri:ios:build": "tauri ios build --config src-tauri/tauri.ios.conf.json",
    "init:android": "bun run tauri:android:init",
    "dev:android": "bun run tauri:android:dev",
    "build:android": "bun run tauri:android:build",
    "init:ios": "bun run tauri:ios:init",
    "dev:ios": "bun run tauri:ios:dev",
    "build:ios": "bun run tauri:ios:build",
    "build:mobile:web-dist": "node ./scripts/build-mobile-web-dist.mjs",
    "build:sidecar": "node ./scripts/build-sidecar.mjs",
    "build": "bun -e \"process.exit(0)\"",
    "type-check": "bun -e \"process.exit(0)\"",
    "lint": "bun -e \"process.exit(0)\""
  },
  "dependencies": {},
  "devDependencies": {
    "@tauri-apps/cli": "^2",
    "@types/node": "^24.3.1",
    "typescript": "~5.8.3"
  }
}
